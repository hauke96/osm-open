{
  "name": "osm-open",
  "version": "1.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0",
    "start:prod": "ng serve --configuration production",
    "build:prod": "ng build --configuration production --base-href \"/osm-open/\"",
    "build:stage": "ng build --configuration production --base-href \"/osm-open-stage/\"",
    "watch": "ng build --watch --configuration development",
    "test": "jest --detectOpenHandles",
    "test:coverage": "jest --coverage",
    "format:check": "prettier --check \"src/{environment,app,assets}/**/*{.js,.ts,.html,.scss,.css,.json}\" && ng lint",
    "format:fix": "prettier --write \"src/{environment,app,assets}/**/*{.js,.ts,.html,.scss,.css,.json}\" && ng lint --fix",
    "update": "npx npm-check-updates -u && npm install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^18.2.13",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/router": "^18.2.13",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "bootstrap-icons": "^1.11.3",
    "ng-mocks": "^14.13.2",
    "ol": "^10.3.1",
    "opening_hours": "^3.8.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.8.1",
    "zone.js": "^0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular-eslint/builder": "^18.4.3",
    "@angular-eslint/eslint-plugin": "^18.4.3",
    "@angular-eslint/eslint-plugin-template": "^18.4.3",
    "@angular-eslint/schematics": "^18.4.3",
    "@angular-eslint/template-parser": "^18.4.3",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.13",
    "@types/jasmine": "^5.1.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.10",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jasmine-core": "^5.5.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.1",
    "prettier": "^3.4.2",
    "typescript": "^5.5.4"
  }
}
